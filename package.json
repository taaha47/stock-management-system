    "sqlite3": "^4.0.7",
    "typeorm": "^0.2.18",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vue-uuid": "^1.1.1",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.1",
    "vuex-module-decorators": "^0.9.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
